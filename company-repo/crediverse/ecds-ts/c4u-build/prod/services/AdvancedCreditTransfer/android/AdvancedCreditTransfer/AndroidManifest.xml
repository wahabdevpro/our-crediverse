<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.concurrent.credittransfer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".CreditTransferActivity"
            android:label="@string/service_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SubscribeActivity"
            android:label="@string/title_activity_subscribe" >
        </activity>
        <activity
            android:name=".TransferActivity"
            android:label="@string/title_activity_transfer" >
        </activity>
        <activity
            android:name=".RecipientsActivity"
            android:label="@string/title_activity_recipients" >
        </activity>
        <activity
            android:name=".AddRecipientActivity"
            android:label="@string/title_activity_add_recipient" >
        </activity>
        <activity
            android:name=".RecipientActivity"
            android:label="@string/title_activity_recipient" >
        </activity>
        <activity
            android:name=".AdjustTransferActivity"
            android:label="@string/title_activity_adjust_transfer" >
        </activity>
    </application>

</manifest>
