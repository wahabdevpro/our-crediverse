							<div class="row">
								<div class="col-sm-6" style="text-align:center;">
									<label class="light-label">{{i18n "transactions.amount"}}</label>		
									<h2 style="margin-top:10px;"><strong>{{#if this.amount}}{{ i18nNum this.amount }}{{else}}<span class="no-data">-</span>{{/if}}</strong></h2>
								</div>	
								<div class="col-sm-6" style="text-align:center;">
									<label class="light-label">{{i18n "transactions.bonusAmount"}}</label>		
									<h2 style="margin-top:10px;"><strong>{{#if this.bonusAmount}}{{ i18nNum this.bonusAmount }}{{else}}<span class="no-data">-</span>{{/if}}</strong></h2>
								</div>	
							</div>
							-->
            			</div>
            			<!-- /.box-body -->
       				</div>
				</div>
				<div class="col-lg-9 col-md-8 col-sm-6">
					<div class="nav-tabs-custom">
  						<ul class="nav nav-tabs">
    						<li class="active"><a href="#transaction" data-toggle="tab">{{i18n "batch.tabBatchDetails"}}</a></li>
  						</ul>
  						<div class="tab-content">
						    <div class="active tab-pane info-panel" id="transaction" style="margin:8px;">
								<div class="row view-fields">
									<div class="col-md-4 col-sm-6">
										<h4>{{i18n "batch.headingDetails"}}</h4>

										<div style="margin-left:10px;">

										<div class="view-field">
											<label>{{i18n "batch.filename"}}</label>
											<div>{{ this.filename }}</div>
										</div>
										<div class="view-field">
											<label>{{i18n "batch.fileSize"}}</label>
											<div>{{ this.fileSize }}</div>
										</div>
										<div class="view-field">
											<label>{{i18n "batch.timestamp"}}</label>
											<div id="batchTimestamp">{{ this.timestamp }}</div>
										</div>
										
										</div>
									</div>
									<div class="col-md-4 col-sm-6">
										<h4>{{i18n "batch.headingOrigin"}}</h4>

										<div style="margin-left:10px;">

										<div class="view-field">
											<label>{{i18n "batch.webUserName"}}</label>
											<div>{{ this.webUserName }}</div>
										</div>
										<div class="view-field">
											<label>{{i18n "batch.coAuthWebUserName"}}</label>
											<div>{{#if this.coAuthWebUserName}}{{ this.coAuthWebUserName }}{{else}}<span class="no-data">{{ i18n "global.notAvailable" }}</span>{{/if}}</div>
										</div>

										<div class="view-field">
											<label>{{i18n "batch.ipAddress"}}</label>
											<div>{{#if this.ipAddress}}{{ this.ipAddress }}{{else}}<span class="no-data">{{ i18n "global.notAvailable" }}</span>{{/if}}</div>
										</div>
										<div class="view-field">
