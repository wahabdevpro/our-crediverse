											<div>{{#if this.deleteCount}}{{ this.deleteCount }}{{else}}<span class="no-data">{{ i18n "global.notAvailable" }}</span>{{/if}}</div>
										</div>
										<div class="view-field">
											<label>{{i18n "batch.failureCount"}}</label>
											<div>{{#if this.failureCount}}{{ this.failureCount }}{{else}}<span class="no-data">{{ i18n "global.notAvailable" }}</span>{{/if}}</div>
										</div>

										</div>
									</div>
								</div>
