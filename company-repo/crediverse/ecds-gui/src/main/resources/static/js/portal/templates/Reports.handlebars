	<!-- SCHEDULE DIALOG -->
	
	<script id="scheduledialogview">
		<div class="modal-header">
		    <button type="button" class="close" data-dismiss="modal"><i class="fa fa-remove"></i> </button>
		    <h4 class="modal-title">{{{dialogTitle}}}</h4>
		</div>
		<div class="modal-body">
			<div class="scheduledetails form-panel">
				<form name="scheduleDetails" class="form-horizontal">
					<div class="row" style="padding-bottom:15px;">
						<label class="col-xs-4 control-label">{{i18n "reports.report"}}</label>
						<div class="col-xs-8">
							<label id="report-name" class="control-label">{{this.reportName}}</label>
						</div>
					</div>	
					<div class="row">
						<div class="col-sm-12">
							{{>modalInput name="description" label="reports.schedule.description" hint="reports.schedule.descriptionHint" value=this.description}}
						</div>
					</div>	
					
					<div class="row form-group required">
						<label class="col-xs-4 control-label">{{i18n "reports.schedule.period"}}</label>
						<div class="col-xs-8">
							<select id="period" name="period" class="form-control">
							   	<option value="HOUR">{{i18n "reports.schedule.periodHOUR"}}</option>
							   	<option value="DAY">{{i18n "reports.schedule.periodDAY"}}</option>
							   	<option value="WEEK">{{i18n "reports.schedule.periodWEEK"}}</option>
							   	<option value="MONTH">{{i18n "reports.schedule.periodMONTH"}}</option>
							</select>
						</div>
					</div>
					
					<div id="timeBlock" class="row form-group" style="display:none;">
						<label class="col-xs-4 control-label">{{i18n "reports.schedule.timeOfDay"}}</label>

						<div class="col-xs-8">
							<div class="row">
								<div class="col-xs-6">
									{{>modalInputField name="timeOfDayString" label="reports.schedule.timeOfDay" hint="reports.schedule.timeOfDayHint"}}
								</div>
							</div>
						</div>
					</div>
					
					<div id="timeIntervalBlock" class="row form-group">
						<label class="col-xs-4 control-label">{{i18n "reports.schedule.timeInterval"}}</label>

						<div class="col-xs-8">
							<div class="row">
								<div class="col-xs-6">
									{{>modalInputField name="startTimeOfDayString" label="reports.schedule.startTimeOfDay" hint="reports.schedule.startTimeOfDayHint"}}
								</div>
								<div class="col-xs-6">
									{{>modalInputField name="endTimeOfDayString" label="reports.schedule.endTimeOfDay" hint="reports.schedule.endTimeOfDayHint"}}
								</div>
							</div>
						</div>
					</div>
				
					<div class="row channel-selection">
						<label class="col-xs-4 control-label">{{i18n "reports.schedule.deliveryChannels"}}</label>
						
						<div class="col-xs-8">
							<div class="form-group row" style="margin-bottom:0;">
								<div class="col-xs-12" style="padding-top:8px;">
									<input id="deliveryChannelEmail" name="deliveryChannelEmail" class="delivery-channel custom-checkbox-fa" value="true" {{#if this.deliveryChannelEmail}}checked="checked"{{/if}} type="checkbox">
									<label for="deliveryChannelEmail">{{i18n "reports.schedule.deliveryChannelEmail"}}</label>
									&nbsp;&nbsp;&nbsp;
									<input id="deliveryChannelSms" name="deliveryChannelSms" class="delivery-channel custom-checkbox-fa" value="true" {{#if this.deliveryChannelSms}}checked="checked"{{/if}} type="checkbox">
									<label for="deliveryChannelSms">{{i18n "reports.schedule.deliveryChannelSms"}}</label>
								</div>
								<div id="deliveryChannelError" class="col-xs-12 help-block" style="display:none; position:relative; top:-7px;">{{i18n "reports.schedule.deliveryChannelNotSelected"}}</div>
							</div>
						</div>
					</div>
					
					<div class="row">
						<label class="col-xs-4 control-label">&nbsp;</label>
						
						<div class="col-xs-8">
							<div class="form-group" style="margin-bottom:0;">
								<div class="col-xs-12" style="padding-top:8px;">
									<input id="emailToAgent" name="emailToAgent" class="custom-checkbox-fa" value="" {{#if this.emailToAgent}}checked="checked"{{/if}} type="checkbox">
									<label for="emailToAgent">{{i18n "reports.schedule.emailToAgent"}}</label>
								</div>
							</div>
						</div>
					</div>	

					<div class="row form-group">
						<label class="col-xs-4 control-label">{{i18n "reports.schedule.webUsers"}}</label>
						
						<div class="col-xs-8">
							<div style="margin-bottom:5px;">
								<input id="user-filter-input" class="form-control" type="text" placeholder="{{i18n "reports.schedule.webUserFilterHint"}}" value=""/>
							</div>
							<div id="agent-users-list" style="height:154px; overflow:auto; padding:5px 10px; border:1px solid #e0e0e0;">
								{{#each userList}}
								{{#ifCond state "!=" "P"}}
								<div class="agent-user-div">
								 	<input id="agent-user-{{id}}" name="agentUserIds[]" value="{{id}}" class="agent-user-selector custom-checkbox-fa" type="checkbox"/>
								 	<label for="agent-user-{{id}}">{{firstName}} {{surname}}</label>
								</div>
							  	{{/ifCond}}
							  	{{/each}}
							</div>
							<div style="margin-top:5px;">
								Showing <strong><span id="agent-users-shown"></span></strong> of <strong><span id="agent-users-total"></span></strong> users, <strong><span id="agent-users-selected"></span></strong> users selected.
							</div>
						</div>
					</div>	
					
					<div class="row form-group">
						<label class="col-xs-4 control-label">{{i18n "reports.schedule.recipientEmails"}}</label>
						<div class="col-xs-8">
							<div id="recipientEmailTemplate" style="display:none;">
							<div class="row">
								<div class="col-xs-12" style="padding-bottom:5px;">
									<div class="input-group">
										<input name="" class="form-control novalidate" value="" placeholder='{{i18n "reports.schedule.recipientEmailsHint"}}' type="email">
										<span class="input-group-btn">
											<a class="btn btn-default scheduleRemoveRecipientButton" type="button"><i class="fa fa-times fa-fw"></i></a>
										</span>
									</div>
								</div>
							</div>	
							</div>
							<div id="recipientEmailsList">
							{{#each this.recipientEmails}}
							<div class="row">
								<div class="col-xs-12" style="padding-bottom:5px;">
									<div class="input-group">
										<input name="recipientEmails[]" class="form-control novalidate" value="{{this}}" placeholder='{{i18n "reports.schedule.recipientEmailsHint"}}' type="email">
										<span class="input-group-btn">
											<a class="btn btn-default scheduleRemoveRecipientButton" type="button"><i class="fa fa-times fa-fw"></i></a>
										</span>
									</div>
								</div>
							</div>	
							{{/each}}
							</div>
		        			<button type="button" class="btn btn-primary scheduleAddRecipientEmailButton">{{i18n "reports.schedule.addRecipientButton"}}</button>
						</div>
					</div>	

					<hr/>
							
					<div class="row">
						<label class="col-xs-4 control-label">&nbsp;</label>
						
						<div class="col-xs-8">
							<div class="form-group" style="margin-bottom:0;">
								<div class="col-xs-12" style="padding-top:8px;">
									<input id="scheduleActive" name="enabled" class="custom-checkbox-fa" value="" {{#if this.enabled}}checked="checked"{{/if}} type="checkbox">
									<label for="scheduleActive">{{i18n "reports.schedule.scheduleEnabled"}}</label>
								</div>
							</div>
						</div>
					</div>	
				</form>
			</div>
		</div>
		<div class="modal-footer">
		    <div class="row form-group">
		        <div class="col-xs-12">
		        	<button type="button" class="btn btn-primary scheduleSubmitButton">{{i18n "reports.schedule.createButton"}}</button>
		        	<button type="button" class="btn btn-default" data-dismiss="modal">{{i18n "reports.schedule.cancelButton"}}</button>
		        </div>
			</div>
		</div>
	</script>

	<!-- SCHEDULE EXECUTE DIALOG -->
	
	<script id="scheduleexecutedialogview">
		<div class="modal-header">
		    <button type="button" class="close" data-dismiss="modal"><i class="fa fa-remove"></i> </button>
		    <h4 class="modal-title">{{{dialogTitle}}}</h4>
		</div>
		<div class="modal-body">
			<div class="scheduledetails form-panel">
				<form name="scheduleDetails" class="form-horizontal">
					<div class="row" style="padding-bottom:15px;">
						<label class="col-xs-4 control-label">{{i18n "reports.report"}}</label>
						<div class="col-xs-8">
							<label id="report-name" class="control-label">{{this.reportName}}</label>
						</div>
					</div>	
					<div class="row form-group required">
						<label class="col-xs-4 control-label">{{i18n "reports.schedule.referenceDate"}}</label>
						<div class="col-xs-3">
							<input type="text" id="referenceDt" name="referenceDt" class="form-control" placeholder="{{i18n "reports.schedule.referenceDateHint"}}"/>
						</div>
						<div class="col-xs-3">
							<input type="text" id="referenceTm" name="referenceTm" class="form-control" placeholder="{{i18n "reports.schedule.referenceDateHint"}}"/>
						</div>
						<label class="col-xs-2 form-control-static">UTC</label>
					</div>	
				</form>

			</div>
		</div>
		<div class="modal-footer">
			<div id="execution-result" class="row" style="text-align:center; display:none;">
				<div class="col-xs-12">
					<div class="alert alert-success"></div>
				</div>
			</div>
		    <div class="row form-group">
		        <div class="col-xs-12">
		        	<button type="button" class="btn btn-primary executeSubmitButton">{{i18n "reports.schedule.executeButton"}}</button>
		        	<button type="button" class="btn btn-default" data-dismiss="modal">{{i18n "reports.schedule.cancelButton"}}</button>
		        </div>
			</div>
		</div>
	</script>

</div>
