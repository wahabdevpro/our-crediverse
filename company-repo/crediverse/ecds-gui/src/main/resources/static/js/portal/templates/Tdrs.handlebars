	<script id="tdrssearchresult">
	    <div class="datatablesview col-xs-12">
	        <div class="box">
	            <div class="box-header">
	                <h3 class="box-title">{{i18n "transactions.transactionSearchResult"}}</h3>
					<div class="pull-right">
						{{#hasPerms "Batch_Download"}}
							<button class="btn btn-primary exportTdrsButton" role="button">{{i18n "global.exportBtn"}}</button>
						{{/hasPerms}}
					</div>
	            </div>
	            <div class="box-body">
		            <table class="tdrstable tableview table table-bordered table-hover table-striped" data-sshash-base='transactionSearch'>
		            </table>
