										<div style="margin-left:30px;">
											<input id="bonus-amount-to" type="text" name="bonusAmountTo" class="form-control" placeholder="{{i18n 'transactions.maxBonus'}}" data-rule-digits="true"/>
										</div>	
									</div>
								</div>	
