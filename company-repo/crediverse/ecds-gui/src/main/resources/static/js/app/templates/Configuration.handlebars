<div>

	
	
<!-- Adjustment screens -->
	<script id="adjustmentsConfig">
		<div class="box">

			{{>configMainHeading heading="config.adjustmentsHeading" btnclass="showAdjustmentDialog" btntext="global.editBtn" permission="Transaction_ConfigAdjust"}}
			
            <form class="form-horizontal">
            	<div class="box-body">
            	
            		{{>configNotificationLabel heading="config.notification" notification=this.notification.texts }}
            		{{>configNotificationLabel heading="config.agentNotification" notification=this.agentNotification.texts }}
            		{{>configNotificationLabel heading="config.batchNotification" notification=this.batchNotification.texts }}
            		
            	</div>
			</form>
			
		</div>
	</script>
	
	<script id="adjustmentsConfigModal">
		{{>configModalHeader heading="config.adjustmentsHeading"}}
		<div class="modal-body">
			<div class="ruledetails">
				<form name="adjustmentConfigForm" class="form-horizontal adjustmentsConfigForm">
				
					{{>configAddVariableNote head="config.tip"}}
				
					{{>configNotificationInput name="notification" heading="config.notification" notification=this.notification}}
					{{>configNotificationInput name="agentNotification" heading="config.agentNotification" notification=this.agentNotification}}
					{{>configNotificationInput name="batchNotification" heading="config.batchNotification" notification=this.batchNotification}}
					
				</form>
			</div>
		</div>
		{{>configModalFooter }}
	</script>
	
	<!-- Balance Enquiries -->

	<script id="balanceEnquiriesConfig">
		<div class="box">

			{{>configMainHeading heading="config.balanceEnqHeading" btnclass="showBalanceEnquiriesConfigDialog" btntext="global.editBtn" permission="Transaction_ConfigBalEnquiry"}}
			
            <form class="form-horizontal">
	        	<div class="box-body">
	        	{{>configSubHeading heading="config.ownBalanceEnqHeading"}}

	        	{{>configSectionHeading heading="config.generalSettings"}}
	        	{{>configLabel label="config.ussdCommand" value=this.ussdCommand.texts.en forceDisplay=true }}
	        	<hr/>

	        	<!--
				{{>configLabel label="Charge" value=this.charge forceDisplay=true}}
				-->
				{{>configNotificationLabel heading="config.smsCommand" notification=this.smsCommand.texts}}
				{{>configNotificationLabel heading="config.notification" notification=this.notification.texts}}
				
				{{>configSubHeading heading="config.agentBalanceEnqHeading"}}
				{{>configSectionHeading heading="config.generalSettings"}}
				{{>configLabelBoolean name="onlyOwnersMayQueryRetailers" label="config.onlyOwnersMayQueryRetailers" value=this.onlyOwnersMayQueryRetailers trueValue="config.yes" falseValue="config.no" }}
				{{>configLabel label="config.ussdCommand" value=this.ussdForOthersCommand.texts.en forceDisplay=true }}
	        	<hr/>
				{{>configNotificationLabel heading="config.smsCommand" notification=this.smsForOthersCommand.texts}}
				{{>configNotificationLabel heading="config.notification" notification=this.notificationForOther.texts}}
	        	<hr/>
	        	</div>
			</form>
			
		</div>
	</script>
	
	<script id="balanceEnquiriesConfigModal">
		{{>configModalHeader heading="config.balanceEnqHeading"}}
		<div class="modal-body">
			<div class="ruledetails">
				<form name="adjustmentConfigForm" class="form-horizontal balanceEnquiriesConfigForm">
					{{>configAddVariableNote}}
					
					{{>configSubHeading heading="config.ownBalanceEnqHeading"}}
					<h5 class="configDialogSectionHeading">{{i18n "config.generalSettings"}}</h5>
					{{>configEnglishInput name="ussdCommand" heading="config.ussdCommand" notification=this.ussdCommand}}
					
					<!--
	      			{{>configInput name="charge" label="Charge" hint="Charge" value=this.charge}}
					-->
				
					{{>configNotificationInput name="smsCommand" heading="config.smsCommand" notification=this.smsCommand}}
					
					
					
					{{>configNotificationInput name="notification" heading="config.notification" notification=this.notification}}
					
					
					
					{{>configSubHeading heading="config.agentBalanceEnqHeading"}}
					<h5 class="configDialogSectionHeading">{{i18n "config.generalSettings"}}</h5>
					{{>booleanRadioInput name="onlyOwnersMayQueryRetailers" label="config.onlyOwnersMayQueryRetailers" trueLabel="config.yes" falseLabel="config.no" value=this.onlyOwnersMayQueryRetailers}}
					
					{{>configEnglishInput name="ussdForOthersCommand" heading="config.ussdCommand" notification=this.ussdForOthersCommand}}
					{{>configNotificationInput name="smsForOthersCommand" heading="config.smsCommand" notification=this.smsForOthersCommand}}
					{{>configNotificationInput name="notificationForOther" heading="config.notification" notification=this.notificationForOther}}
					
				</form>
			</div>
		</div>
		{{>configModalFooter }}
	</script>
	
	<!-- Batch Configuration -->
	<script id="batchConfig">
		<div class="box">

			{{>configMainHeading heading="config.batchHeading" btnclass="showBatchConfigDialog" btntext="global.editBtn" permission="Transaction_ConfigBatch"}}
	
            <form class="form-horizontal">
	        	<div class="box-body">

					{{>configSectionHeading heading="config.generalSettings"}}
					{{>configLabel label="config.batchArchiveFolder" value=this.batchArchiveFolder forceDisplay=true}}
					{{>configLabel label="config.batchEntriesRetentionDays" value=this.batchEntriesRetentionDays forceDisplay=true}}
					{{>configLabel label="config.batchDownloadChunkSize" value=this.batchDownloadChunkSize forceDisplay=true}}
					<hr/>

	        	</div>
			</form>
			
		</div>
	</script>
	
	<script id="batchConfigModal">
		{{>configModalHeader heading="config.batchHeading"}}
		<div class="modal-body">
			<div class="ruledetails">
				<form name="adjustmentConfigForm" class="form-horizontal batchConfigForm">

					<h5 class="configDialogSectionHeading">{{i18n "config.generalSettings"}}</h5>
	      			{{>configInput name="batchArchiveFolder" label="config.batchArchiveFolder" hint="config.batchArchiveFolderHint" value=this.batchArchiveFolder}}
	      			{{>configInput name="batchEntriesRetentionDays" label="config.batchEntriesRetentionDays" hint="config.batchEntriesRetentionDays" value=this.batchEntriesRetentionDays}}
	      			{{>configInput name="batchDownloadChunkSize" label="config.batchDownloadChunkSize" hint="config.batchDownloadChunkSize" value=this.batchDownloadChunkSize}}
				
				</form>
			</div>
		</div>
		{{>configModalFooter }}
	</script>
	
