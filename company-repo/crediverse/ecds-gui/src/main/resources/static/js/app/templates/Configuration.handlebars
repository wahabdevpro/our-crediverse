	<!-- Analytics Configuration -->
	<script id="analyticsConfig">
		<div class="box">
			{{>configMainHeading heading="config.analytics.heading" btnclass="showAnalyticsConfigDialog" btntext="global.editBtn" permission="Analytics_Configure"}}
			
			<div class="nav-tabs-custom">
				<!-- Nav tabs -->
				<ul class="nav nav-tabs" role="tablist">
					<li role="presentation" class="active">
