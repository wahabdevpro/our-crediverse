	<!-- Mobile Number Format Config -->
	<script id="mobileNumberFormat">
		<div class="box">
			{{>configMainHeading heading="config.msisdnTransformation.pageTitle" permission="General_GeneralConfig"}}
			
			<div class="nav-tabs-custom">
				<!-- Nav tabs -->
				<ul class="nav nav-tabs" role="tablist">
					<li role="presentation" class="active">
						<a href="#mapping" aria-controls="mapping" role="tab" data-toggle="tab">{{i18n "Config-Mapping"}}</a>
					</li>
					<li role="presentation">
						<a href="#msisdn-transformation-tool-tab" aria-controls="config" role="tab" data-toggle="tab">{{i18n "Tool"}}</a>
					</li>
				</ul>
			
			<!-- Tab panes -->
				<div class="tab-content">
				<div role="tabpanel" class="tab-pane active" id="mapping">
						<form class="form-horizontal">
						<div class="box-body">
		        			<div class="msisdn-transformation">	        	
				        		{{>configSectionHeading heading="MSISDN Transformation Rule"}}
								<table class="table table-bordered table-hover table-striped dataTable dtr-inline" style="margin: 0px !important;">
									<thead>
										<tr role="row">
											<th id="oprcodes">Operator code(s)</th>
											<th class="col-md-1" id="prepend">Prepend</th>
											<th id="btn" style="text-align: center">Action</th>
										</tr>
									</thead>
									<tbody id="mappingTable">
										<tr role="row" class="mapping-row">
											<td ><input class="mapping-old-codes" type="text" value="{{this.value}}"></td>
											<td class="col-md-1"><input class="mapping-new-prefix" type="text" value="{{this.key}}"></td>
										</tr>
									</tbody>
									<tfoot>
    									<tr role="row">
    										<td id="oprcodes">&nbsp;</td>
    										<td class="col-md-1" id="prepend">&nbsp;</td>
    										<td class="col-md-1" id="btn" style="text-align: center">
    											<button type="button" id="addMappingButton" class="btn btn-primary btn-xs"><span class="glyphicon glyphicon-plus"></span> Add</button>
											</td>
    									</tr>
    								</tfoot>
								</table>
								
								<div class="box-body">
					        		<div class="form-group">
					        			<label class="col-sm-2 control-label">{{i18n "config.msisdnTransformation.oldNumbersLengthLabel"}}</label>
										<input id="oldMobileNumberLengthId" type="text" class="col-sm-1">
			        				</div>
			        				
			        				<h5 class="configDialogSectionHeading">{{i18n "config.msisdnTransformation.wrongNumberFormatMessages"}}</h5>
									<div class="form-group">
										<label class="col-sm-2 control-label">English</label>
										<div>
											<textarea id="wrongFormatEnMessageId" style="width: 365px; height: 87px;"></textarea>
										</div>
										
										<label class="col-sm-2 control-label">French</label>
										<div>
											<textarea id="wrongFormatFrMessageId" style="width: 365px; height: 87px;"></textarea>
										</div>
									</div>
			        			</div>
			        			
								<div class="pull-right">
									<button type="button" class="btn btn-primary" id="save-config-button-id">Save</button>
				        		</div>
			        		</div>
			        	</div>
					</form>
				</div>
				
				<div role="tabpanel" class="tab-pane" id="msisdn-transformation-tool-tab">
					<form class="form-horizontal">
						<div id="tool-main-div-id" class="box-body">
							{{>configSectionHeading heading="config.msisdnTransformation.toolHeadline"}}
						
							{{>normalButtonWithDescriptionTemplate elementId="msisdn-transformation-run-wrapper-id"
										buttonId="msisdn-transformation-run-button-id"
										buttonCaption="config.msisdnTransformation.runButtonCaption"
										buttonDescription="config.msisdnTransformation.runButtonDescription" }}
										
							<textarea id="mobile-transformation-console-id"
								readonly="readonly"
								style="margin: 0px; height: 180px; width: 1050px;"></textarea>
						
							{{>normalButtonWithDescriptionTemplate elementId="msisdn-transformation-dual-phase-wrapper-id"
										buttonId="msisdn-transformation-dual-phase-button-id"
										buttonCaption="config.msisdnTransformation.dualPhasButtonCaption"
										buttonDescription="config.msisdnTransformation.dualPhasButtonDescription" }}
						</div>
					</form>
				</div>

				</div>
				
			</div>
		</div>
	</script>

	<!-- Last Transaction Enquiries -->
	<script id="lastTransactionEnquiriesConfig">
		<div class="box">

			{{>configMainHeading heading="config.lastTransQryHeading" btnclass="showLastTransactionEnquiriesConfigDialog" btntext="global.editBtn" permission="Transaction_ConfigTransEnq"}}
	
			<div class="nav-tabs-custom">
				<ul class="nav nav-tabs" role="tablist">
					<li role="presentation" class="active">
						<a href="#lastTransactionGeneral" aria-controls="lastTransactionGeneral" role="tab" data-toggle="tab">{{i18n "config.lastTransactionGeneralTab"}}</a>
					</li>
			    	<li role="presentation">
			    		<a href="#lastTransactionMultipleResults" aria-controls="lastTransactionMultipleResults" role="tab" data-toggle="tab">{{i18n "config.lastTransactionMultiResultsTab"}}</a>
			    	</li>
				</ul>
		
				<!-- Tab panes -->
				<div class="tab-content">
					<div role="tabpanel" class="tab-pane active" id="lastTransactionGeneral">
			            <form class="form-horizontal">
				        	<div class="box-body">
				        		{{>configSectionHeading heading="config.generalSettings"}}
				        		{{>configLabel label="config.ussdCommand" value=this.ussdCommand.texts.en forceDisplay=true }}
				        		
								<!--
								{{>configLabel name="charge" label="Charge" hint="Charge" value=this.charge forceDisplay=true}}
								<hr/>
								-->
				
								<hr/>
								{{>configNotificationLabel heading="config.smsCommand" notification=this.smsCommand.texts}}
								{{>configNotificationLabel heading="config.notification" notification=this.notification.texts}}				
							
				        	</div>
						</form>
					</div>
					<div role="tabpanel" class="tab-pane" id="lastTransactionMultipleResults">
						<form class="form-horizontal">
		        			<div class="box-body">
		        				{{>configSectionHeading heading="config.generalSettings"}}
		        				{{>configLabel label="config.maxTransactions" value=this.maxTransactions forceDisplay=true}}
		        				{{#ifCond this.ordering "==" "N+"}}
		        					{{>configLabel label="config.transactionOrder" value=(i18n "enums.transactionOrdering.number") forceDisplay=true}}
		        				{{/ifCond}}
		        				{{#ifCond this.ordering "==" "T+"}}
		        					{{>configLabel label="config.transactionOrder" value=(i18n "enums.transactionOrdering.time") forceDisplay=true}}
		        				{{/ifCond}}
		        				{{#ifCond this.ordering "==" "S+"}}
		        					{{>configLabel label="config.transactionOrder" value=(i18n "enums.transactionOrdering.status") forceDisplay=true}}
		        				{{/ifCond}}
		        				
		        				<hr/>
		        				{{>configNotificationLabel heading="config.listNotification" notification=this.listNotification.texts}}
		        				{{>configNotificationLabel heading="config.transactionLine" notification=this.transactionLine.texts}}
		        				
		        				{{>configNotificationLabel heading="config.tagSuccessfulLabel" notification=this.successful.texts}}
		        				{{>configNotificationLabel heading="config.tagFailedLabel" notification=this.failed.texts}}
		        				{{>configNotificationLabel heading="config.tagPendingLabel" notification=this.pending.texts}}		        				
		        			</div>
		        		</form>
					</div>
				</div>
			</div>
		</div>
	</script>	
	
	<script id="lastTransactionEnquiriesConfigModal">
		{{>configModalHeader heading="config.lastTransQryHeading"}}
		<div class="modal-body">
			<div class="ruledetails">
				<form name="adjustmentConfigForm" class="form-horizontal lastTransactionEnquiriesConfigForm">
					{{>configAddVariableNote}}
					
					<div class="nav-tabs-custom">
						<ul class="nav nav-tabs" role="tablist">
							<li role="presentation" class="active">
								<a href="#transactionGeneralModal" aria-controls="transactionGeneralModal" role="tab" data-toggle="tab">{{i18n "config.generalTransactionTab"}}</a>
							</li>
							<li role="presentation">
								<a href="#lastTransactionMultipleResultsModal" aria-controls="lastTransactionMultipleResultsModal" role="tab" data-toggle="tab">{{i18n "config.lastTransactionMultiResultsTab"}}</a>
							</li>
						</ul>
						<!-- Tab panes -->
						<div class="tab-content">
							<div role="tabpanel" class="tab-pane active" id="transactionGeneralModal">
							
								<h5 class="configDialogSectionHeading">{{i18n "config.generalSettings"}}</h5>
								{{>configEnglishInput name="ussdCommand" heading="config.ussdCommand" notification=this.ussdCommand}}
								
								<!--
				      			{{>configInput name="charge" label="Charge" hint="Charge" value=this.charge}}
				      			-->
				      			
								{{>configNotificationInput name="smsCommand" heading="config.smsCommand" notification=this.smsCommand}}
								{{>configNotificationInput name="notification" heading="config.notification" notification=this.notification}}
							</div>
							<div role="tabpanel" class="tab-pane" id="lastTransactionMultipleResultsModal">			
								<h5 class="configDialogSectionHeading">{{i18n "config.generalSettings"}}</h5>
								{{>configInput name="maxTransactions" label="config.maxTransactions" hint="config.maxTransactions" value=this.maxTransactions}}
								{{>configSelect name="ordering" label="config.transactionOrder" data="N+:enums.transactionOrdering.number,T+:enums.transactionOrdering.time,S+:enums.transactionOrdering.status" value=this.ordering }}
								<hr/>
								{{>configNotificationInput name="listNotification" heading="config.listNotification" notification=this.listNotification}}
								{{>configNotificationInput name="transactionLine" heading="config.transactionLine" notification=this.transactionLine}}
								
								{{>configNotificationInput name="successful" heading="config.tagSuccessfulLabel" notification=this.successful}}
								{{>configNotificationInput name="failed" heading="config.tagFailedLabel" notification=this.failed}}
								{{>configNotificationInput name="pending" heading="config.tagPendingLabel" notification=this.pending}}
							</div>
						</div>
					</div>
				</form>
			</div>
		</div>
		{{>configModalFooter }}
	</script>	
	
